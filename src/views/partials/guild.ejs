<div class="box">
  <article class="media">
    <div class="media-left">
      <figure class="image is-64x64">
        <img
          src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>"
          alt=""
        />
      </figure>
    </div>
    <div class="media-content">
      <div class="content">
        <p>
          <strong> <%= guild.name %> </strong>
        </p>
      </div>
    </div>
  </article>
</div>
